{
  "manifest_version": 2,
  "name": "Safe Browsing Extension",
  "version": "1.0.0",
  "description": "This extension blocks unsafe URLs using the Google Safe Browsing API.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  }
}
