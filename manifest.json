{
    "name": "SafeNet",
    "version": "1.0",
    "description": "Blocks malicious links using the Google Safe Browsing API",
    "manifest_version": 3,
    "permissions": [
      "safeBrowsing",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "https://safebrowsing.googleapis.com/",
      "https://www.googleapis.com/"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      "google_safe_browsing.json"
    ]
  }
  